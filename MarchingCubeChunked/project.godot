config_version=5

[application]

config/name="Marching Cube Chunked"
config/description="Moon surface demo. Refer control on control map."
run/main_scene="res://Main/viewer.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Fullscreen="*res://AutoLoad/fullscreen.gd"

[debug]

gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_screen=1
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=2

[file_customization]

folder_colors={
"res://addons/": "red"
}

[filesystem]

import/blender/enabled=false

[global_group]

CHUNKS="Marching Cube Chunks"
DEBUG="Collection of Debug meshes"

